alter table instructor add HireDate date;

INSERT INTO INSTRUCTOR(instructor_id, name, dept_id, is_hod, salary, HireDate) VALUES (10,'John Doe',1,'Y',80000,date '2018-06-30');
INSERT INTO INSTRUCTOR(instructor_id, name, dept_id, is_hod, salary, HireDate) VALUES (12,'Jane Smith',1,'N',75000,date '2021-03-01');
INSERT INTO INSTRUCTOR(instructor_id, name, dept_id, is_hod, salary, HireDate) VALUES (13,'Bob Johnson',2,'Y',82000,date '2023-11-01');

select * from instructor where hiredate< date '2025-06-30' and hiredate > date '2019-12-31';
select name, salary from instructor where length(salary)>=4;

#GET to know the more -less-exact salary 
SELECT
    instructor_id,
    name,
    salary,
    CASE
        WHEN salary < 2000 THEN 'Less salary'
        WHEN salary = 2000 THEN 'Exact salary'
        WHEN salary > 2000 THEN 'More salary'
    END AS salary_status
FROM instructor;

CREATE TABLE departments (
    deptno INT PRIMARY KEY,
    dept_name VARCHAR2(50),
    location VARCHAR2(50)
);

