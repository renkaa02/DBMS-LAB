select * from instructor where hiredate< date '2025-06-30' and hiredate > date '2019-12-31';
select name, salary from instructor where length(salary)>=4;

#GET to know the more -less-exact salary 
SELECT
    instructor_id,
    name,
    salary,
    CASE
        WHEN salary < 2000 THEN 'Less salary'
        WHEN salary = 2000 THEN 'Exact salary'
        WHEN salary > 2000 THEN 'More salary'
    END AS salary_status
FROM instructor;

