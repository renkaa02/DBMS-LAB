DROP TABLE TAKES PURGE;
DROP TABLE SECTION PURGE;
DROP TABLE COURSE PURGE;
DROP TABLE INSTRUCTOR PURGE;
DROP TABLE STUDENT PURGE;
DROP TABLE DEPARTMENT PURGE;
DROP TABLE TIME_SLOT PURGE;
DROP TABLE CLASSROOM PURGE;
DROP TABLE UNIVERSITY PURGE;

CREATE TABLE UNIVERSITY (
    university_id NUMBER PRIMARY KEY,
    name VARCHAR2(100) NOT NULL
);

INSERT INTO UNIVERSITY VALUES (1, 'Example University');
INSERT INTO UNIVERSITY VALUES (2, 'Another University');
INSERT INTO UNIVERSITY VALUES (3, 'Third University');
INSERT INTO UNIVERSITY VALUES (4, 'Fourth University');
INSERT INTO UNIVERSITY VALUES (5, 'Fifth University');
INSERT INTO UNIVERSITY VALUES (6, 'Sixth University');
INSERT INTO UNIVERSITY VALUES (7, 'Seventh University');



CREATE TABLE DEPARTMENT (
    dept_id NUMBER PRIMARY KEY,
    name VARCHAR2(50) NOT NULL,
    building VARCHAR2(50),
    budget NUMBER(12,2),
    university_id NUMBER,
    CONSTRAINT fk_dept_univ FOREIGN KEY (university_id)
        REFERENCES UNIVERSITY(university_id)
);

INSERT INTO DEPARTMENT VALUES (1, 'Computer Science', 'Taylor', 100000, 1);
INSERT INTO DEPARTMENT VALUES (2, 'Mathematics', 'Watson', 90000, 1);
INSERT INTO DEPARTMENT VALUES (3, 'Physics', 'Newton', 95000, 1);
INSERT INTO DEPARTMENT VALUES (4, 'Biology', 'Darwin', 85000, 1);
INSERT INTO DEPARTMENT VALUES (5, 'Chemistry', 'Curie', 92000, 1);
INSERT INTO DEPARTMENT VALUES (6, 'English', 'Shakespeare', 80000, 1);
INSERT INTO DEPARTMENT VALUES (7, 'History', 'Roosevelt', 78000, 1);


CREATE TABLE INSTRUCTOR (
    instructor_id NUMBER PRIMARY KEY,
    name VARCHAR2(50) NOT NULL,
    dept_id NUMBER,
    is_hod CHAR(1) DEFAULT 'N' CHECK (is_hod IN ('Y','N')),
    salary NUMBER(10,2),
    CONSTRAINT fk_instr_dept FOREIGN KEY (dept_id)
        REFERENCES DEPARTMENT(dept_id)
);

INSERT INTO INSTRUCTOR VALUES (1, 'John Doe', 1, 'Y', 80000);
INSERT INTO INSTRUCTOR VALUES (2, 'Jane Smith', 1, 'N', 75000);
INSERT INTO INSTRUCTOR VALUES (3, 'Bob Johnson', 2, 'Y', 82000);
INSERT INTO INSTRUCTOR VALUES (4, 'Alice Brown', 2, 'N', 76000);
INSERT INTO INSTRUCTOR VALUES (5, 'Charlie Davis', 3, 'Y', 81000);
INSERT INTO INSTRUCTOR VALUES (6, 'Eva Wilson', 3, 'N', 77000);
INSERT INTO INSTRUCTOR VALUES (7, 'Frank Miller', 4, 'Y', 79000);

CREATE TABLE COURSE (
    course_id VARCHAR2(8) PRIMARY KEY,
    title VARCHAR2(100) NOT NULL,
    dept_id NUMBER,
    credits NUMBER,
    CONSTRAINT fk_course_dept FOREIGN KEY (dept_id)
        REFERENCES DEPARTMENT(dept_id)
);

INSERT INTO COURSE VALUES ('CS-101', 'Introduction to Programming', 1, 3);
INSERT INTO COURSE VALUES ('CS-201', 'Data Structures', 1, 4);
INSERT INTO COURSE VALUES ('MATH-101', 'Calculus I', 2, 4);
INSERT INTO COURSE VALUES ('MATH-201', 'Linear Algebra', 2, 3);
INSERT INTO COURSE VALUES ('PHY-101', 'Mechanics', 3, 4);
INSERT INTO COURSE VALUES ('PHY-201', 'Quantum Physics', 3, 4);
INSERT INTO COURSE VALUES ('BIO-101', 'Introduction to Biology', 4, 4);


CREATE TABLE SECTION (
    section_id NUMBER PRIMARY KEY,
    course_id VARCHAR2(8),
    sec_id NUMBER,
    semester VARCHAR2(6),
    acad_year NUMBER,
    building VARCHAR2(50),
    room_number VARCHAR2(10),
    time_slot_id VARCHAR2(4),
    CONSTRAINT fk_section_course FOREIGN KEY (course_id)
        REFERENCES COURSE(course_id)
);

INSERT INTO SECTION VALUES (1,'CS-101',1,'Fall',2023,'Taylor','3128','A');
