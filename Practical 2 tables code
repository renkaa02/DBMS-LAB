CREATE TABLE DEPARTMENT (dept_id INT PRIMARY KEY, name VARCHAR(50) NOT NULL, building VARCHAR(50), buget DECIMAL(12,2), university_id INT, FOREIGN KEY (university_id) REFERENCE UNIVERSITY(university_id));

