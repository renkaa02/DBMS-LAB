CREATE TABLE COURSE (course_id VARCHAR(8) PRIMARY KEY, title VARCHAR(100) NOT NULL, dept_id INT, credits INT, FOREIGN KEY (dept_id) REFERENCES DEPARTMENT(dept_id));

