 1. select * from tab;
 2. desc st_info;
 3. select max(prn) from st_info;
 4. create sequence st_sq start with 108 increment by 1;
 5. insert into st_info (prn,name, m1,m2,m3) values (st_sq.nextval,'xa108',09,10,11);



to create sequence:
create sequence st_sq start with 108 increment by 1;

create sequence emp_sq start with 1 increment by 1;

create table emp (emp_id int primary key, name varchar2(20));

